{
    "python.linting.enabled": false,
    "python.analysis.typeCheckingMode": "basic",

    "emmet.includeLanguages": {
        "django-html": "html"
    },
    "[django-html]": {},
    "files.associations": {
        "*.html": "django-html"
    },
    "[python]": {
        "editor.defaultFormatter": "ms-python.autopep8"
    },
    "python.formatting.provider": "none"
}
